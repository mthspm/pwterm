      "code_cache" : "AppData\\Local\\Google\\Chrome\\User Data\\Default\\Code Cache",
      "gpu_cache" : "AppData\\Local\\Google\\Chrome\\User Data\\Default\\GPUCache"
    }
  },
  "discord" : {
    "user_profile" : true,
    "system_drive" : false,
    "paths" : {
      "cache" : "AppData\\Roaming\\discord\\Cache",
      "code_cache" : "AppData\\Roaming\\discord\\Code Cache",
      "gpu_cache" : "AppData\\Roaming\\discord\\GPUCache"
    }
  },
  "temp" : {
    "user_profile" : true,
    "system_drive" : false,
    "paths" : {
